{
  "name": "locat-web-tracker",
  "rewrites": [
    {
      "source": "/locat",
      "destination": "/index.html"
    },
    {
      "source": "/locat/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/locat/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/locat/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}